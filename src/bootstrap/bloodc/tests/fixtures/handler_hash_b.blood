
effect Counter {
    op inc() -> i32;
}

deep handler HandlerBeta for Counter {
    let mut count: i32
    return(x) { x + 100 }
    op inc() {
        count = count + 1;
        resume(count)
    }
}

fn main() -> i32 {
    with HandlerBeta { count: 0 } handle {
        perform Counter.inc()
    }
}
