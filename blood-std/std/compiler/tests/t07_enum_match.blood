// Test: enum definition and pattern matching
// EXPECT: 1
// EXPECT: 99
// EXPECT: 0
pub enum Value {
    Int(i32),
    Pair(i32, i32),
    None,
}

fn main() -> i32 {
    let a = Value::Int(1);
    let b = Value::Pair(99, 200);
    let c = Value::None;

    match a {
        Value::Int(n) => { println_int(n); }
        Value::Pair(x, y) => { println_int(x + y); }
        Value::None => { println_int(-1); }
    }

    match b {
        Value::Int(n) => { println_int(n); }
        Value::Pair(x, y) => { println_int(x); }
        Value::None => { println_int(-1); }
    }

    match c {
        Value::Int(n) => { println_int(n); }
        Value::Pair(x, y) => { println_int(x + y); }
        Value::None => { println_int(0); }
    }
    0
}
