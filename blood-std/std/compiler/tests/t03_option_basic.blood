// Test: Option creation and matching
// EXPECT: 42
// EXPECT: none
fn main() -> i32 {
    let x: Option<i32> = Option::Some(42);
    match x {
        Option::Some(val) => { println_int(val); }
        Option::None => { println("none"); }
    }

    let y: Option<i32> = Option::None;
    match y {
        Option::Some(val) => { println_int(val); }
        Option::None => { println("none"); }
    }
    0
}
