// Test program for pipe operator
fn double(x: i32) -> i32 {
    x * 2
}

fn add_ten(x: i32) -> i32 {
    x + 10
}

fn main() {
    // Test: 5 |> double => double(5) => 10
    let a: i32 = 5 |> double;
    println_int(a);

    // Test: 3 |> add_ten => add_ten(3) => 13
    let b: i32 = 3 |> add_ten;
    println_int(b);

    // Test chained: 2 |> double |> add_ten => add_ten(double(2)) => add_ten(4) => 14
    let c: i32 = 2 |> double |> add_ten;
    println_int(c);
}
