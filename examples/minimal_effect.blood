// Minimal effect test to isolate type mismatch

effect Counter {
    op inc() -> i32;
}

deep handler BasicCounter for Counter {
    let mut count: i32

    return(x) { x }

    op inc() {
        count = count + 1;
        resume(count)
    }
}

fn main() -> i32 {
    with BasicCounter { count: 0 } handle {
        perform Counter.inc()
    }
}
