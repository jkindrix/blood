// EXPECT: 3
// EXPECT: 7
// EXPECT: 5
// EXPECT: 10
// EXPECT: 1
// EXPECT: -1
// EXPECT: 0
// Tests that new prelude items are auto-imported.
// Note: Each function tested in isolation to avoid first_gen codegen limitation
// with multiple u32-parameter calls in the same function body.
mod std;

fn test_min_u32() {
    let a = min_u32(3, 7);
    print_int(a as i32);
    print_str("\n");
}

fn test_max_u32() {
    let b = max_u32(3, 7);
    print_int(b as i32);
    print_str("\n");
}

fn test_min_usize() {
    let c = min_usize(5, 10);
    print_int(c as i32);
    print_str("\n");
}

fn test_max_usize() {
    let d = max_usize(5, 10);
    print_int(d as i32);
    print_str("\n");
}

fn test_sign_positive() {
    let s1 = sign(42);
    print_int(s1);
    print_str("\n");
}

fn test_sign_negative() {
    let s2 = sign(0 - 5);
    print_int(s2);
    print_str("\n");
}

fn test_sign_zero() {
    let s3 = sign(0);
    print_int(s3);
    print_str("\n");
}

fn main() -> i32 {
    test_min_u32();
    test_max_u32();
    test_min_usize();
    test_max_usize();
    test_sign_positive();
    test_sign_negative();
    test_sign_zero();
    0
}
